#!/bin/bash
# @name:      PyHouse_Install/bin/start_pyhouse
# @author:    D. Brian Kimmel
# @contact:   D.BrianKimmel@gmail.com
# @copyright: (c) 2015-2015 by D. Brian Kimmel
# @license:   MIT License
# @note:      Created on Oct 19, 2015
# @Summary:   This starts PyHouse

WORK=/home/pyhouse/workspace/PyHouse/

echo "Starting the PyHouse system"
cd ~
> nohup.out
nohup python ${WORK}src/PyHouse.py  1>/dev/null 2>&1 </dev/null &
cd

### END DBK
